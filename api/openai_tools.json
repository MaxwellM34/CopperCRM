          },
          "estimated_total_cost_usd": {
            "type": "number",
            "title": "Estimated Total Cost Usd"
          },
          "model": {
            "type": "string",
            "title": "Model"
          },
          "sample_size": {
            "type": "integer",
            "title": "Sample Size"
          }
        },
        "type": "object",
        "required": [
          "pending",
          "generated",
          "average_cost_usd",
          "estimated_total_cost_usd",
          "model",
          "sample_size"
        ],
        "title": "EmailStats"
      },
      "GenerateRequest": {
        "properties": {
          "count": {
            "anyOf": [
              {
                "type": "integer",
                "maximum": 500.0,
                "minimum": 1.0
              },
              {
                "type": "null"
              }
            ],
            "title": "Count",
            "description": "How many leads to generate emails for (omit to use all pending)."
          }
        },
        "type": "object",
        "title": "GenerateRequest"
      },
      "GenerateResult": {
        "properties": {
          "attempted": {
            "type": "integer",
            "title": "Attempted"
          },
          "generated": {
            "type": "integer",
            "title": "Generated"
          },
          "pending_after": {
            "type": "integer",
            "title": "Pending After"
          },
          "total_cost_usd": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Total Cost Usd"
          },
          "model": {
            "type": "string",
            "title": "Model"
          },
          "errors": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Errors"
          }
        },
        "type": "object",
        "required": [
          "attempted",
          "generated",
          "pending_after",
          "total_cost_usd",
          "model"
