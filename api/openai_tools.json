            "default": 0
          },
          "leads_updated": {
            "type": "integer",
            "title": "Leads Updated",
            "default": 0
          },
          "skipped": {
            "type": "integer",
            "title": "Skipped",
            "default": 0
          },
          "errors": {
            "items": {
              "$ref": "#/components/schemas/ImportRowError"
            },
            "type": "array",
            "title": "Errors"
          }
        },
        "type": "object",
        "title": "LeadCompanyImportResult"
